    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-hapi": "^8.0.0",
    "eslint-plugin-hapi": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.10.0",
